{
  "name": "vue-boilerplate",
  "version": "1.1.0",
  "main": "index.js",
  "repository": "https://github.com/peteringram0/vue-boilerplate",
  "author": "Peter Ingram <peter.ingram0@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prod": "NODE_ENV=production webpack --progress --hide-modules --config webpack.config.js",
    "dev": "NODE_ENV=development webpack --progress --hide-modules --config webpack.config.js",
    "serve": "webpack-dev-server --open --inline --hot",
    "test:fn": "NODE_ENV=test nyc ava --verbose --no-cache --serial --fail-fast"
  },
  "ava": {
    "require": [
      "ignore-styles",
      "babel-register",
      "./test/helpers/setup.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "ava": "^0.21.0",
    "avoriaz": "^2.4.3",
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-vue-jsx": "^3.2.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.24.0",
    "browser-env": "^3.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "live-server": "^1.2.0",
    "nyc": "^11.0.3",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "postcss-loader": "^2.0.6",
    "pug": "^2.0.0-rc.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "template-html-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "vue-loader": "^12.2.2",
    "vue-node": "^1.1.1",
    "vue-template-compiler": "^2.4.1",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "vue": "^2.4.1",
    "vue-router": "^2.7.0"
  }
}
